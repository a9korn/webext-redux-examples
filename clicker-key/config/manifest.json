{
  "manifest_version": 3,
  "name": "Webext Redux Example",
  "description": "Web Extension and Redux Example",
  "version": "0.0.0.1",
  "background": {
    "service_worker": "service-worker.js"
  },
  "browser_action": {
    "default_title": "Webext Redux Example",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": ["content-script.js"]
    }
  ]
}
